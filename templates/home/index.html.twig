{% extends 'base.html.twig' %}
{% from 'partials/_icons.html.twig' import check, linkedin, twitter, star %}

{% block title %}ProService — Accueil{% endblock %}

{% block body %}
    {# Hero Section #}
    <section class="hero">
        <video class="hero__video" autoplay muted loop playsinline>
            <source src="{{ asset('videos/hero.mp4') }}" type="video/mp4">
        </video>
        <div class="hero__overlay"></div>
        <div class="hero__content">
            <h1 class="hero__title">Votre Partenaire de Confiance</h1>
            <p class="hero__subtitle">Des solutions professionnelles sur mesure pour accélérer votre succès</p>
            <div class="hero__actions">
                <a href="#" class="btn btn--primary">Réserver maintenant</a>
                <a href="#" class="btn btn--outline">Voir nos réalisations</a>
            </div>
        </div>
    </section>

    {# Section Ils nous font confiance #}
    <section class="trust">
        <div class="container">
            <p class="trust__label">Ils nous font confiance</p>
        </div>
        <div class="trust__slider">
            <div class="trust__track">
                {% for i in 1..2 %}
                {# Orange #}
                <div class="trust__item">
                    <svg viewBox="0 0 120 40" xmlns="http://www.w3.org/2000/svg">
                        <rect x="8" y="8" width="24" height="24" rx="4" fill="#FF7900"/>
                        <text x="40" y="27" font-family="Inter, Arial, sans-serif" font-size="16" font-weight="700" fill="#FF7900">Orange</text>
                    </svg>
                </div>
                {# SNCF #}
                <div class="trust__item">
                    <svg viewBox="0 0 110 40" xmlns="http://www.w3.org/2000/svg">
                        <rect x="4" y="6" width="102" height="28" rx="4" fill="#0C1B5E"/>
                        <text x="55" y="26" text-anchor="middle" font-family="Inter, Arial, sans-serif" font-size="17" font-weight="800" fill="#fff" letter-spacing="2">SNCF</text>
                    </svg>
                </div>
                {# Michelin #}
                <div class="trust__item">
                    <svg viewBox="0 0 140 40" xmlns="http://www.w3.org/2000/svg">
                        <text x="10" y="28" font-family="Inter, Arial, sans-serif" font-size="18" font-weight="700" fill="#002B5C">MICHELIN</text>
                        <line x1="10" y1="32" x2="130" y2="32" stroke="#FDD835" stroke-width="3"/>
                    </svg>
                </div>
                {# Decathlon #}
                <div class="trust__item">
                    <svg viewBox="0 0 160 40" xmlns="http://www.w3.org/2000/svg">
                        <circle cx="16" cy="20" r="10" fill="none" stroke="#0066CC" stroke-width="3"/>
                        <path d="M10 20 L16 14 L22 20" fill="none" stroke="#0066CC" stroke-width="2.5" stroke-linecap="round" stroke-linejoin="round"/>
                        <text x="32" y="27" font-family="Inter, Arial, sans-serif" font-size="16" font-weight="700" fill="#0066CC">Decathlon</text>
                    </svg>
                </div>
                {# Bouygues #}
                <div class="trust__item">
                    <svg viewBox="0 0 150 40" xmlns="http://www.w3.org/2000/svg">
                        <ellipse cx="16" cy="20" rx="12" ry="12" fill="#009FE3"/>
                        <text x="34" y="27" font-family="Inter, Arial, sans-serif" font-size="16" font-weight="700" fill="#003D6B">Bouygues</text>
                    </svg>
                </div>
                {# Carrefour #}
                <div class="trust__item">
                    <svg viewBox="0 0 150 40" xmlns="http://www.w3.org/2000/svg">
                        <path d="M8 20 L18 8 L18 32 Z" fill="#004E9A"/>
                        <path d="M28 20 L18 8 L18 32 Z" fill="#E3001B"/>
                        <text x="34" y="27" font-family="Inter, Arial, sans-serif" font-size="16" font-weight="700" fill="#004E9A">Carrefour</text>
                    </svg>
                </div>
                {# TotalEnergies #}
                <div class="trust__item">
                    <svg viewBox="0 0 190 40" xmlns="http://www.w3.org/2000/svg">
                        <circle cx="16" cy="20" r="10" fill="#FF0000"/>
                        <circle cx="16" cy="20" r="6" fill="#FF6600"/>
                        <circle cx="16" cy="20" r="3" fill="#FFD700"/>
                        <text x="32" y="27" font-family="Inter, Arial, sans-serif" font-size="15" font-weight="700" fill="#E30613">Total</text>
                        <text x="78" y="27" font-family="Inter, Arial, sans-serif" font-size="15" font-weight="700" fill="#00A651">Energies</text>
                    </svg>
                </div>
                {# Airbus #}
                <div class="trust__item">
                    <svg viewBox="0 0 120 40" xmlns="http://www.w3.org/2000/svg">
                        <text x="10" y="28" font-family="Inter, Arial, sans-serif" font-size="18" font-weight="700" fill="#00205B" letter-spacing="1.5">AIRBUS</text>
                        <line x1="10" y1="32" x2="110" y2="32" stroke="#00205B" stroke-width="1.5"/>
                    </svg>
                </div>
                {% endfor %}
            </div>
        </div>
    </section>

    {# Section Nos Prestations #}
    <section class="services" id="services">
        <div class="container">
            <div class="section-header">
                <h2 class="section-title">Nos Prestations</h2>
                <p class="section-subtitle">Des services complets pour répondre à tous vos besoins</p>
            </div>
            <div class="services__grid" data-stagger>

                <div class="services__card">
                    <div class="services__icon services__icon--blue">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                            <path d="M3 9l9-7 9 7v11a2 2 0 01-2 2H5a2 2 0 01-2-2V9z"/>
                            <polyline points="9 22 9 12 15 12 15 22"/>
                        </svg>
                    </div>
                    <h3 class="services__name">Conseil Stratégique</h3>
                    <p class="services__desc">Développez votre entreprise avec nos experts en stratégie d'affaires et pilotage de croissance.</p>
                    <a href="#" class="arrow-link">En savoir plus <span>&rarr;</span></a>
                </div>

                <div class="services__card">
                    <div class="services__icon services__icon--indigo">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                            <path d="M17 21v-2a4 4 0 00-4-4H5a4 4 0 00-4 4v2"/>
                            <circle cx="9" cy="7" r="4"/>
                            <path d="M23 21v-2a4 4 0 00-3-3.87"/>
                            <path d="M16 3.13a4 4 0 010 7.75"/>
                        </svg>
                    </div>
                    <h3 class="services__name">Gestion de Projet</h3>
                    <p class="services__desc">Pilotage et coordination de vos projets complexes, de la conception à la livraison.</p>
                    <a href="#" class="arrow-link">En savoir plus <span>&rarr;</span></a>
                </div>

                <div class="services__card">
                    <div class="services__icon services__icon--violet">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                            <path d="M22 10v6M2 10l10-5 10 5-10 5z"/>
                            <path d="M6 12v5c0 2 3 3 6 3s6-1 6-3v-5"/>
                        </svg>
                    </div>
                    <h3 class="services__name">Formation Pro</h3>
                    <p class="services__desc">Programmes de formation sur mesure pour vos équipes, certifiants et opérationnels.</p>
                    <a href="#" class="arrow-link">En savoir plus <span>&rarr;</span></a>
                </div>

                <div class="services__card">
                    <div class="services__icon services__icon--emerald">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                            <polyline points="22 12 18 12 15 21 9 3 6 12 2 12"/>
                        </svg>
                    </div>
                    <h3 class="services__name">Optimisation</h3>
                    <p class="services__desc">Améliorez vos processus et votre performance grâce à nos méthodes éprouvées.</p>
                    <a href="#" class="arrow-link">En savoir plus <span>&rarr;</span></a>
                </div>

                <div class="services__card">
                    <div class="services__icon services__icon--amber">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                            <circle cx="12" cy="12" r="3"/>
                            <line x1="12" y1="2" x2="12" y2="5"/>
                            <line x1="12" y1="19" x2="12" y2="22"/>
                            <line x1="2" y1="12" x2="5" y2="12"/>
                            <line x1="19" y1="12" x2="22" y2="12"/>
                            <line x1="4.93" y1="4.93" x2="7.05" y2="7.05"/>
                            <line x1="16.95" y1="16.95" x2="19.07" y2="19.07"/>
                            <line x1="4.93" y1="19.07" x2="7.05" y2="16.95"/>
                            <line x1="16.95" y1="7.05" x2="19.07" y2="4.93"/>
                        </svg>
                    </div>
                    <h3 class="services__name">Innovation</h3>
                    <p class="services__desc">Transformez vos idées en solutions concrètes grâce à notre accompagnement innovation.</p>
                    <a href="#" class="arrow-link">En savoir plus <span>&rarr;</span></a>
                </div>

                <div class="services__card">
                    <div class="services__icon services__icon--rose">
                        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round">
                            <path d="M9 12l2 2 4-4"/>
                            <path d="M12 2a10 10 0 110 20 10 10 0 010-20z"/>
                        </svg>
                    </div>
                    <h3 class="services__name">Audit & Conformité</h3>
                    <p class="services__desc">Assurez votre conformité réglementaire et sécurisez vos activités durablement.</p>
                    <a href="#" class="arrow-link">En savoir plus <span>&rarr;</span></a>
                </div>

            </div>
            <button class="section-toggle" id="services-toggle">
                Voir plus <span>&darr;</span>
            </button>
        </div>
    </section>

    {# Section Pourquoi nous choisir #}
    <section class="why" id="about">
        <div class="container">
            <div class="why__grid">
                <div class="why__content reveal reveal--left">
                    <h2 class="why__title">Pourquoi nous choisir ?</h2>
                    <p class="why__text">
                        Avec plus de 15 ans d'expérience, nous accompagnons les entreprises dans leur
                        transformation et leur croissance. Notre approche personnalisée garantit des résultats
                        mesurables.
                    </p>
                    <ul class="why__list">
                        <li>{{ check() }} Plus de 500 projets réalisés avec succès</li>
                        <li>{{ check() }} Équipe d'experts certifiés et expérimentés</li>
                        <li>{{ check() }} Accompagnement personnalisé et suivi continu</li>
                        <li>{{ check() }} Satisfaction client de 98%</li>
                    </ul>
                    <a href="#" class="btn btn--primary why__cta why__cta--desktop">Découvrir notre histoire</a>
                </div>
                <div class="why__media reveal reveal--right">
                    <div class="why__video-wrapper">
                        <video class="why__video" autoplay muted loop playsinline>
                            <source src="{{ asset('videos/team.mp4') }}" type="video/mp4">
                        </video>
                    </div>
                </div>
                <a href="#" class="btn btn--primary why__cta why__cta--mobile">Découvrir notre histoire</a>
            </div>
        </div>
    </section>

    {# Section Galerie #}
